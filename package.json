    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.18",
    "mongoose-unique-validator": "^2.0.3"
  },
  "devDependencies": {
